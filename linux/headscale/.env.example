# Headscale 部署配置
# 复制此文件为 .env 并修改相应配置

# 服务器公网 IP 或域名（必填）
SERVER_HOST=your-server-ip-or-domain

# 端口配置
HEADSCALE_PORT=8080
HEADSCALE_GRPC_PORT=50443
HEADSCALE_METRICS_PORT=9090
HEADSCALE_STUN_PORT=3478

# UI 端口
UI_HTTP_PORT=8008
UI_API_PORT=8081

# Caddy 端口
CADDY_HTTP_PORT=80
CADDY_HTTPS_PORT=443

# DERP 服务器配置
DERP_REGION_ID=999
DERP_REGION_CODE=headscale
DERP_REGION_NAME="Headscale Embedded"

# UI Basic Auth 认证 (保护管理界面)
UI_AUTH_USER=admin
UI_AUTH_PASSWORD=headscale

# 网络配置
NETWORK_SUBNET=172.28.0.0/16
HEADSCALE_IP=172.28.0.10
HEADSCALE_UI_IP=172.28.0.20
CADDY_IP=172.28.0.30
